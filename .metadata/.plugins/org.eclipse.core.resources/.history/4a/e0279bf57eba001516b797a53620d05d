import java.util.Arrays;

public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static int singleNumber (int[] nums){
		int result = 0;
		Arrays.sort(nums);
		for (int i = 0;i<nums.length-1;i+=2){
			if(nums[i]!= nums[i+1]){
				result = nums[i];
			}
			else{
				continue;
			}
		}
		return result;
	}

}
